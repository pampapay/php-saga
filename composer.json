{
    "name": "pampapay/php-saga",
    "description": "Simple library to implement saga pattern for synchronous commands",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pampapay\\PhpSaga\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pampapay\\PhpSaga\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Javier Trejo",
            "email": "trejo.javier@gmail.com"
        }
